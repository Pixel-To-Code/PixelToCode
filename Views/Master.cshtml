@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@using FlowJo
@{
    Layout = null;
    int cachedSeconds = 10;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <link rel="icon" href="/favicon.ico">

    @Html.CachedPartial("Header", Model, cachedSeconds, cacheByPage: true)

    <!-- Bootstrap core CSS 
    <link href="/css/bootstrap.min.css" rel="stylesheet">
        -->

    <link href="/css/responsive.min.css" rel="stylesheet" />
    <link href="~/Css/navigation.css" rel="stylesheet" />

    <link href="/css/styles.css" rel="stylesheet">

    <!--[if lt IE 9 &!(IEMobile)]>
        <script src="/js/html5shiv.min.js"></script>
        <script src="/js/respond.min.js"></script>
    <![endif]-->


    @*<!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <script src="../../assets/js/ie10-viewport-bug-workaround.js"></script>
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Bootstrap core JavaScript
      ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="/js/jquery.min.js"></script>
    <script src="/js/jquery.validate.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>*@


    <!--[if IE 8]>
    <script src="/js/jquery-1.11.1.min.js"></script>
    <![endif]-->
    <!--[if gt IE 8]><!-->
    <script src="/js/jquery-2.1.1.min.js"></script>
    <!--<![endif]-->

    <script src="/js/responsive.js"></script>
    
    <script src="/js/main.js"></script>


</head>
<body>

    <div id="header-full" class="fullwidth clearfix">
        <div id="header-full-inner">
            <nav role="navigation">
                <div id="header-container" class="@Grid.Container">
                    <div class="@Grid.Row">
                        <div class="@Grid.Cols(Grid.Xs, 12)">
                            @Html.CachedPartial("TopNav", Model, cachedSeconds, cacheByPage: true)
                        </div>
                    </div>
                </div>
            </nav>
        </div>
    </div>
    @*<div id="breadcrumb-full" class="fullwidth clearfix">
        <div id="breadcrumb-full-inner">
            <div id="breadcrumb" class="@Grid.Container">
                @Html.CachedPartial("Breadcrumb", Model, cachedSeconds, cacheByPage: true)
            </div>
        </div>
    </div>*@
    <div id="content-full" class="fullwidth clearfix">
        <div id="content-full-inner">
            <div id="content" class="@Grid.Container">
                @RenderBody()
            </div>
        </div>
    </div>
    <div id="footer-full" class="fullwidth clearfix">
        <div id="footer-full-inner">
            <div id="footer" class="@Grid.Container">
                @Html.CachedPartial("FooterNav", Model, cachedSeconds, cacheByPage: true)
            </div>
        </div>
    </div>

    @Html.CachedPartial("GoogleAnalytics", Model, cachedSeconds, cacheByPage: false)



    @* FOF *@
    <script>
        document.documentElement.className = 'js'; // adds class="js" to <html> element
        function uncheckboxes(nav) {
            var navarray = document.getElementsByName(nav);
            for (var i = 0; i < navarray.length; i++) {
                navarray[i].checked = false
            }
        }
    </script>



</body>
</html>
